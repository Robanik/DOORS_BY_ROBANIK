AddButton("Esp", "Esp Closet", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø GARDR–ûBE
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local camera = Workspace.CurrentCamera

-- –°–æ–∑–¥–∞–µ–º ESP —Ç–µ–∫—Å—Ç
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(0, 255, 255)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 18
    label.Font = Enum.Font.FredokaOne -- –∫—Ä–∞—Å–∏–≤—ã–π —à—Ä–∏—Ñ—Ç

    billboard.Parent = object
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –≤—Å–µ—Ö –∫–æ–º–Ω–∞—Ç
local function setupESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Assets") then
            local assets = room.Assets
            if assets:FindFirstChild("Wardrobe") then
                local wardrobe = assets.Wardrobe:FindFirstChild("Main")
                if wardrobe and not wardrobe:FindFirstChild("BillboardGui") then
                    createBillboard(wardrobe, "(Closet | " .. i .. ")")
                end
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupESP()
    end
end)
end)

AddButton("Esp", "Esp Doors", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø –î–í–ï–†–ï–ô
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –°–æ–∑–¥–∞–µ–º ESP —Ç–µ–∫—Å—Ç
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 4, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 170, 0) -- –æ—Ä–∞–Ω–∂–µ–≤—ã–π (–∫–∞–∫ —Å–≤–µ—Ç —É –¥–≤–µ—Ä–∏)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne -- —Å—Ç–∏–ª—å–Ω—ã–π —à—Ä–∏—Ñ—Ç

    billboard.Parent = object
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –¥–≤–µ—Ä–µ–π
local function setupDoorESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local doorModel = room.Door:FindFirstChild("Door")
            if doorModel and not doorModel:FindFirstChild("BillboardGui") then
                createBillboard(doorModel, "(Door | " .. i .. ")")
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupDoorESP()
    end
end)
end)

AddButton("Esp", "Esp Key", function()
-- // –†–ê–ë–û–ß–ò–ô ESP –î–õ–Ø –í–°–ï–• –ö–õ–Æ–ß–ï–ô
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text, color)
    if object and object:IsA("BasePart") and not object:FindFirstChild("BillboardGui") then
        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = object
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true

        local label = Instance.new("TextLabel")
        label.Parent = billboard
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = text
        label.TextColor3 = color or Color3.fromRGB(0, 255, 0)
        label.TextStrokeTransparency = 0.2
        label.TextSize = 20
        label.Font = Enum.Font.FredokaOne

        billboard.Parent = object
    end
end

-- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫ –≤—Å–µ—Ö –æ–±—ä–µ–∫—Ç–æ–≤ –∫–ª—é—á–µ–π –≤ –∫–æ–º–Ω–∞—Ç–µ
local function findKeys(parent)
    for _, child in pairs(parent:GetChildren()) do
        -- –ï—Å–ª–∏ –æ–±—ä–µ–∫—Ç –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è KeyHitbox
        if child.Name == "KeyHitbox" and child:IsA("BasePart") then
            createBillboard(child, "(Key)", Color3.fromRGB(0, 255, 0))
        end

        -- –ï—Å–ª–∏ –æ–±—ä–µ–∫—Ç —Å–æ–¥–µ—Ä–∂–∏—Ç KeyObtain –∏ –µ—Å—Ç—å Hitbox –≤–Ω—É—Ç—Ä–∏
        if child.Name == "KeyObtain" and child:IsA("Folder") or child:IsA("Model") then
            local hitbox = child:FindFirstChild("Hitbox")
            if hitbox and hitbox:FindFirstChild("KeyHitbox") then
                createBillboard(hitbox.KeyHitbox, "(Key)", Color3.fromRGB(0, 200, 255))
            end
        end

        -- –†–µ–∫—É—Ä—Å–∏–≤–Ω–æ –ø—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ –¥–æ—á–µ—Ä–Ω–∏–µ –æ–±—ä–µ–∫—Ç—ã
        findKeys(child)
    end
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è ESP
local function setupKeyESP()
    if not Workspace:FindFirstChild("CurrentRooms") then return end

    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room then
            findKeys(room)
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 1 —Å–µ–∫—É–Ω–¥—É
task.spawn(function()
    while true do
        setupKeyESP()
        task.wait(1)
    end
end)
end)

AddButton("Esp", "Esp Lever", function()
-- // ESP –î–õ–Ø LEVER (–Ω–µ —Å–≤—è–∑–∞–Ω–Ω—ã–π —Å –∫–ª—é—á–∞–º–∏)
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text, color)
    if object:FindFirstChild("BillboardGui") then return end
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 2, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = color or Color3.fromRGB(255, 165, 0) -- –æ—Ä–∞–Ω–∂–µ–≤—ã–π –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne

    billboard.Parent = object
end

-- –ù–æ–≤—ã–π ESP –¥–ª—è LeverForGate.Main
local function setupLeverESP()
    if not Workspace:FindFirstChild("CurrentRooms") then return end

    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Assets") then
            local lever = room.Assets:FindFirstChild("LeverForGate")
            if lever and lever:FindFirstChild("Main") then
                createBillboard(lever.Main, "(Lever | "..i..")", Color3.fromRGB(255, 165, 0))
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 1 —Å–µ–∫—É–Ω–¥—É
task.spawn(function()
    while true do
        setupLeverESP()
        task.wait(1)
    end
end)
end)

AddButton("Esp", "Esp Lock", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø LOCK
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 50, 50) -- –∫—Ä–∞—Å–Ω—ã–π (–ø–æ–¥ –∑–∞–º–æ–∫)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne

    billboard.Parent = object
end

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—Å–µ—Ö –∫–æ–º–Ω–∞—Ç
local function setupLockESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local door = room.Door
            if door:FindFirstChild("Lock") then
                local lock = door.Lock
                if lock and not lock:FindFirstChild("BillboardGui") then
                    createBillboard(lock, "(Lock | " .. i .. ")")
                end
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupLockESP()
    end
end)
end)

AddButton("Esp", "Esp Players", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

local BoxColor = Color3.fromRGB(0, 255, 0)

-- –°–æ–∑–¥–∞–Ω–∏–µ Drawing –æ–±—ä–µ–∫—Ç–∞
local function createDrawing(type, props)
    local obj = Drawing.new(type)
    for i, v in pairs(props) do
        obj[i] = v
    end
    return obj
end

-- ESP –¥–ª—è –∏–≥—Ä–æ–∫–∞
local function createESP(player)
    local box = createDrawing("Square", {
        Thickness = 1.5,
        Color = BoxColor,
        Filled = false,
        Visible = false
    })

    local tracer = createDrawing("Line", {
        Thickness = 1.5,
        Color = BoxColor,
        Visible = false
    })

    local nameText = createDrawing("Text", {
        Size = 14,
        Center = true,
        Outline = true,
        Color = Color3.fromRGB(0, 255, 0),
        Visible = false
    })

    local hpText = createDrawing("Text", {
        Size = 13,
        Center = true,
        Outline = true,
        Color = Color3.fromRGB(255, 0, 0),
        Visible = false
    })

    RunService.RenderStepped:Connect(function()
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local root = player.Character.HumanoidRootPart
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health > 0 then
                -- –ü–æ–∑–∏—Ü–∏–∏ –≥–æ–ª–æ–≤—ã –∏ –Ω–æ–≥
                local head = player.Character:FindFirstChild("Head")
                if head then
                    local headPos, headVisible = Camera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.5, 0))
                    local legPos, legVisible = Camera:WorldToViewportPoint(root.Position - Vector3.new(0, 3, 0))

                    if headVisible or legVisible then
                        local boxHeight = math.abs(headPos.Y - legPos.Y)
                        local boxWidth = boxHeight / 2
                        local boxX = headPos.X - boxWidth / 2
                        local boxY = headPos.Y

                        -- –ë–æ–∫—Å
                        box.Visible = true
                        box.Size = Vector2.new(boxWidth, boxHeight)
                        box.Position = Vector2.new(boxX, boxY)
                        box.Color = BoxColor

                        -- –ù–∏–∫
                        nameText.Visible = true
                        nameText.Text = player.Name
                        nameText.Position = Vector2.new(headPos.X, headPos.Y - 15)

                        -- HP
                        hpText.Visible = true
                        hpText.Text = string.format("HP: %d", math.floor(humanoid.Health))
                        hpText.Position = Vector2.new(headPos.X, legPos.Y + 5)

                        -- –õ–∏–Ω–∏—è (tracer)
                        tracer.Visible = true
                        tracer.From = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y) -- –Ω–∏–∑ —ç–∫—Ä–∞–Ω–∞
                        tracer.To = Vector2.new(root.Position.X, root.Position.Y)
                        tracer.To = Vector2.new((headPos.X + (headPos.X + boxWidth)) / 2, legPos.Y) -- —Ü–µ–Ω—Ç—Ä –±–æ–∫—Å–∞
                        tracer.Color = BoxColor
                    else
                        box.Visible = false
                        tracer.Visible = false
                        nameText.Visible = false
                        hpText.Visible = false
                    end
                end
            else
                box.Visible = false
                tracer.Visible = false
                nameText.Visible = false
                hpText.Visible = false
            end
        else
            box.Visible = false
            tracer.Visible = false
            nameText.Visible = false
            hpText.Visible = false
        end
    end)
end

-- –ü–æ–¥–∫–ª—é—á–∞–µ–º ESP –∫–æ –≤—Å–µ–º –∏–≥—Ä–æ–∫–∞–º
for _, plr in pairs(Players:GetPlayers()) do
    if plr ~= LocalPlayer then
        createESP(plr)
    end
end

Players.PlayerAdded:Connect(function(plr)
    if plr ~= LocalPlayer then
        createESP(plr)
    end
end)
end)

------------------------------------------------

AddButton("Info", "Rush", function()
-- LocalScript

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = player:WaitForChild("PlayerGui")

local TextLabel = Instance.new("TextLabel")
TextLabel.Size = UDim2.new(1, 0, 0.1, 0) -- —à–∏—Ä–∏–Ω–∞ = –≤–µ—Å—å —ç–∫—Ä–∞–Ω, –≤—ã—Å–æ—Ç–∞ = 10%
TextLabel.Position = UDim2.new(0, 0, 0.9, 0) -- —Å–Ω–∏–∑—É —ç–∫—Ä–∞–Ω–∞
TextLabel.BackgroundTransparency = 1
TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
TextLabel.TextStrokeTransparency = 0.2
TextLabel.TextScaled = true
TextLabel.Font = Enum.Font.GothamBold
TextLabel.Visible = false
TextLabel.Parent = ScreenGui

-- —Ñ—É–Ω–∫—Ü–∏—è –ø–æ–∫–∞–∑–∞ —Ç–µ–∫—Å—Ç–∞
local function ShowRushWarning()
	TextLabel.Text = "‚ö†Ô∏è –†–∞—à –∏–¥—ë—Ç!"
	TextLabel.Visible = true
	game:GetService("TweenService"):Create(
		TextLabel,
		TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
		{TextTransparency = 0}
	):Play()
end

-- —Ñ—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è —Ç–µ–∫—Å—Ç–∞
local function HideRushWarning()
	game:GetService("TweenService"):Create(
		TextLabel,
		TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In),
		{TextTransparency = 1}
	):Play()
	wait(0.5)
	TextLabel.Visible = false
end

-- –æ—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º Rush
local RushFolder = game.Workspace:WaitForChild("RushMoving")

RushFolder.ChildAdded:Connect(function(child)
	if child.Name == "RushNew" then
		ShowRushWarning()
		child.AncestryChanged:Connect(function(_, parent)
			if not parent then
				HideRushWarning()
			end
		end)
	end
end)
end)

------------------------------------------------

AddButton("Light", "FullBright", function()
-- // FULLBRIGHT
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Lighting = game:GetService("Lighting")

local function enableFullBright()
    -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å—Ç–∞—Ä—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ (–Ω–∞ —Å–ª—É—á–∞–π –æ—Ç–∫–ª—é—á–µ–Ω–∏—è)
    if not Lighting:FindFirstChild("OriginalSettings") then
        local folder = Instance.new("Folder")
        folder.Name = "OriginalSettings"
        folder.Parent = Lighting

        local ambient = Instance.new("Color3Value")
        ambient.Name = "Ambient"
        ambient.Value = Lighting.Ambient
        ambient.Parent = folder

        local brightness = Instance.new("NumberValue")
        brightness.Name = "Brightness"
        brightness.Value = Lighting.Brightness
        brightness.Parent = folder

        local outdoor = Instance.new("Color3Value")
        outdoor.Name = "OutdoorAmbient"
        outdoor.Value = Lighting.OutdoorAmbient
        outdoor.Parent = folder
    end

    -- –í–∫–ª—é—á–∞–µ–º FullBright
    Lighting.Ambient = Color3.fromRGB(255, 255, 255)
    Lighting.Brightness = 2
    Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
end

local function disableFullBright()
    local folder = Lighting:FindFirstChild("OriginalSettings")
    if folder then
        Lighting.Ambient = folder.Ambient.Value
        Lighting.Brightness = folder.Brightness.Value
        Lighting.OutdoorAmbient = folder.OutdoorAmbient.Value
        folder:Destroy()
    end
end

-- –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è
enableFullBright() -- –≤–∫–ª—é—á–∏—Ç—å FullBright
-- disableFullBright() -- –æ—Ç–∫–ª—é—á–∏—Ç—å –∏ –≤–µ—Ä–Ω—É—Ç—å —Å—Ç–∞—Ä—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
end)

