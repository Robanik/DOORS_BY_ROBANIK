AddButton("Esp", "Esp Lock", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- –°–æ–∑–¥–∞—ë–º ScreenGui –¥–ª—è ESP
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = PlayerGui

-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è ESP –∑–∞–º–∫–æ–≤
local locksESP = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP –¥–ª—è –∑–∞–º–∫–∞
local function createLockESP(lock)
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 120, 0, 50)
    textLabel.BackgroundTransparency = 0.5
    textLabel.BackgroundColor3 = Color3.fromRGB(0,0,0)
    textLabel.TextColor3 = Color3.fromRGB(255,255,255)
    textLabel.TextScaled = true
    textLabel.Parent = screenGui
    return textLabel
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –≤—Å–µ—Ö –∑–∞–º–∫–æ–≤ –æ—Ç –¥–≤–µ—Ä–µ–π 0 –¥–æ 100
local function updateLocks()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local door = room.Door
            if door:FindFirstChild("Lock") then
                local lock = door.Lock
                if not locksESP[lock] then
                    locksESP[lock] = createLockESP(lock)
                end
            end
        end
    end
end

-- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP –∑–∞–º–∫–æ–≤
RunService.RenderStepped:Connect(function()
    updateLocks() -- –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –∑–∞–º–∫–æ–≤

    for lock, espLabel in pairs(locksESP) do
        if lock and lock.Parent then
            local lockValue = lock.Value
            espLabel.Text = "Lock: " .. math.clamp(lockValue, 0, 100) -- –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –æ—Ç 0 –¥–æ 100

            local lockPos = lock.Position
            local screenPos, onScreen = workspace.CurrentCamera:WorldToScreenPoint(lockPos)
            espLabel.Visible = onScreen
            if onScreen then
                espLabel.Position = UDim2.new(0, screenPos.X - 60, 0, screenPos.Y - 25)
            end
        end
    end
end)
end)

AddButton("Esp", "Esp Doors", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- –°–æ–∑–¥–∞—ë–º ScreenGui –¥–ª—è ESP
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = PlayerGui

-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è ESP –¥–≤–µ—Ä–µ–π
local doorsESP = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP –¥–ª—è –¥–≤–µ—Ä–∏
local function createDoorESP(door, doorNumber)
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 150, 0, 50)
    textLabel.BackgroundTransparency = 0.5
    textLabel.BackgroundColor3 = Color3.fromRGB(0,0,0)
    textLabel.TextColor3 = Color3.fromRGB(255,255,255)
    textLabel.TextScaled = true
    textLabel.Text = "Door | " .. doorNumber
    textLabel.Parent = screenGui
    return textLabel
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏—è ESP –¥–≤–µ—Ä–µ–π –æ—Ç 0 –¥–æ 100
local function updateDoors()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local door = room.Door
            if door:FindFirstChild("Door") and not doorsESP[door] then
                doorsESP[door] = createDoorESP(door.Door, i)
            end
        end
    end
end

-- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP –¥–≤–µ—Ä–µ–π
RunService.RenderStepped:Connect(function()
    updateDoors() -- –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –¥–≤–µ—Ä–µ–π

    for door, espLabel in pairs(doorsESP) do
        if door and door.Parent then
            local doorPos = door.Position
            local screenPos, onScreen = workspace.CurrentCamera:WorldToScreenPoint(doorPos)
            espLabel.Visible = onScreen
            if onScreen then
                espLabel.Position = UDim2.new(0, screenPos.X - 75, 0, screenPos.Y - 25)
            end
        end
    end
end)
end)

AddButton("Esp", "Esp Keys", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- –°–æ–∑–¥–∞—ë–º ScreenGui –¥–ª—è ESP
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = PlayerGui

-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è ESP –∫–ª—é—á–µ–π
local keysESP = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP –¥–ª—è –∫–ª—é—á–∞
local function createKeyESP(key, keyNumber)
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 150, 0, 50)
    textLabel.BackgroundTransparency = 0.5
    textLabel.BackgroundColor3 = Color3.fromRGB(0,0,0)
    textLabel.TextColor3 = Color3.fromRGB(255,255,0) -- –∂—ë–ª—Ç—ã–π –¥–ª—è –∫–ª—é—á–µ–π
    textLabel.TextScaled = true
    textLabel.Text = "Key | " .. keyNumber
    textLabel.Parent = screenGui
    return textLabel
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏—è ESP –∫–ª—é—á–µ–π –æ—Ç 0 –¥–æ 100
local function updateKeys()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room then
            local success, key = pcall(function()
                return room.Assets.Dresser.DrawerContainer.KeyObtain.Hitbox.KeyHitbox
            end)
            if success and key and not keysESP[key] then
                keysESP[key] = createKeyESP(key, i)
            end
        end
    end
end

-- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP –∫–ª—é—á–µ–π
RunService.RenderStepped:Connect(function()
    updateKeys() -- –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –∫–ª—é—á–µ–π

    for key, espLabel in pairs(keysESP) do
        if key and key.Parent then
            local keyPos = key.Position
            local screenPos, onScreen = workspace.CurrentCamera:WorldToScreenPoint(keyPos)
            espLabel.Visible = onScreen
            if onScreen then
                espLabel.Position = UDim2.new(0, screenPos.X - 75, 0, screenPos.Y - 25)
            end
        end
    end
end)
end)

AddButton("Esp", "Esp Players", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

local BoxColor = Color3.fromRGB(0, 255, 0)

-- –°–æ–∑–¥–∞–Ω–∏–µ Drawing –æ–±—ä–µ–∫—Ç–∞
local function createDrawing(type, props)
    local obj = Drawing.new(type)
    for i, v in pairs(props) do
        obj[i] = v
    end
    return obj
end

-- ESP –¥–ª—è –∏–≥—Ä–æ–∫–∞
local function createESP(player)
    local box = createDrawing("Square", {
        Thickness = 1.5,
        Color = BoxColor,
        Filled = false,
        Visible = false
    })

    local tracer = createDrawing("Line", {
        Thickness = 1.5,
        Color = BoxColor,
        Visible = false
    })

    local nameText = createDrawing("Text", {
        Size = 14,
        Center = true,
        Outline = true,
        Color = Color3.fromRGB(0, 255, 0),
        Visible = false
    })

    local hpText = createDrawing("Text", {
        Size = 13,
        Center = true,
        Outline = true,
        Color = Color3.fromRGB(255, 0, 0),
        Visible = false
    })

    RunService.RenderStepped:Connect(function()
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local root = player.Character.HumanoidRootPart
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health > 0 then
                -- –ü–æ–∑–∏—Ü–∏–∏ –≥–æ–ª–æ–≤—ã –∏ –Ω–æ–≥
                local head = player.Character:FindFirstChild("Head")
                if head then
                    local headPos, headVisible = Camera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.5, 0))
                    local legPos, legVisible = Camera:WorldToViewportPoint(root.Position - Vector3.new(0, 3, 0))

                    if headVisible or legVisible then
                        local boxHeight = math.abs(headPos.Y - legPos.Y)
                        local boxWidth = boxHeight / 2
                        local boxX = headPos.X - boxWidth / 2
                        local boxY = headPos.Y

                        -- –ë–æ–∫—Å
                        box.Visible = true
                        box.Size = Vector2.new(boxWidth, boxHeight)
                        box.Position = Vector2.new(boxX, boxY)
                        box.Color = BoxColor

                        -- –ù–∏–∫
                        nameText.Visible = true
                        nameText.Text = player.Name
                        nameText.Position = Vector2.new(headPos.X, headPos.Y - 15)

                        -- HP
                        hpText.Visible = true
                        hpText.Text = string.format("HP: %d", math.floor(humanoid.Health))
                        hpText.Position = Vector2.new(headPos.X, legPos.Y + 5)

                        -- –õ–∏–Ω–∏—è (tracer)
                        tracer.Visible = true
                        tracer.From = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y) -- –Ω–∏–∑ —ç–∫—Ä–∞–Ω–∞
                        tracer.To = Vector2.new(root.Position.X, root.Position.Y)
                        tracer.To = Vector2.new((headPos.X + (headPos.X + boxWidth)) / 2, legPos.Y) -- —Ü–µ–Ω—Ç—Ä –±–æ–∫—Å–∞
                        tracer.Color = BoxColor
                    else
                        box.Visible = false
                        tracer.Visible = false
                        nameText.Visible = false
                        hpText.Visible = false
                    end
                end
            else
                box.Visible = false
                tracer.Visible = false
                nameText.Visible = false
                hpText.Visible = false
            end
        else
            box.Visible = false
            tracer.Visible = false
            nameText.Visible = false
            hpText.Visible = false
        end
    end)
end

-- –ü–æ–¥–∫–ª—é—á–∞–µ–º ESP –∫–æ –≤—Å–µ–º –∏–≥—Ä–æ–∫–∞–º
for _, plr in pairs(Players:GetPlayers()) do
    if plr ~= LocalPlayer then
        createESP(plr)
    end
end

Players.PlayerAdded:Connect(function(plr)
    if plr ~= LocalPlayer then
        createESP(plr)
    end
end)
end)

AddButton("Esp", "closet", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- –°–æ–∑–¥–∞—ë–º ScreenGui –¥–ª—è ESP
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = PlayerGui

-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è ESP —à–∫–∞—Ñ–æ–≤
local closetsESP = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP –¥–ª—è —à–∫–∞—Ñ–∞
local function createClosetESP(closet, closetNumber)
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 180, 0, 50)
    textLabel.BackgroundTransparency = 0.5
    textLabel.BackgroundColor3 = Color3.fromRGB(0,0,0)
    textLabel.TextColor3 = Color3.fromRGB(0,255,255) -- –≥–æ–ª—É–±–æ–π –¥–ª—è —à–∫–∞—Ñ–æ–≤
    textLabel.TextScaled = true
    textLabel.Text = "Closet | " .. closetNumber
    textLabel.Parent = screenGui
    return textLabel
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏—è ESP —à–∫–∞—Ñ–æ–≤ –æ—Ç 0 –¥–æ 1000
local function updateClosets()
    for i = 0, 1000 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room then
            local success, closet = pcall(function()
                return room.Assets.Wardrobe.Main
            end)
            if success and closet and not closetsESP[closet] then
                closetsESP[closet] = createClosetESP(closet, i)
            end
        end
    end
end

-- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP —à–∫–∞—Ñ–æ–≤
RunService.RenderStepped:Connect(function()
    updateClosets() -- –∞–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö —à–∫–∞—Ñ–æ–≤

    for closet, espLabel in pairs(closetsESP) do
        if closet and closet.Parent then
            local closetPos = closet.Position
            local screenPos, onScreen = workspace.CurrentCamera:WorldToScreenPoint(closetPos)
            espLabel.Visible = onScreen
            if onScreen then
                espLabel.Position = UDim2.new(0, screenPos.X - 90, 0, screenPos.Y - 25)
            end
        end
    end
end)
end)

------------------------------------------------

AddButton("Afk", "Afk Doors Teleport", function()
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –∫ –¥–≤–µ—Ä–∏
local function teleportToDoor(doorNumber)
    local room = Workspace.CurrentRooms:FindFirstChild(tostring(doorNumber))
    if room and room:FindFirstChild("Door") then
        local door = room.Door
        if door:FindFirstChild("Door") then
            HumanoidRootPart.CFrame = door.Door.CFrame + Vector3.new(0, 5, 0) -- —Ç–µ–ª–µ–ø–æ—Ä—Ç —á—É—Ç—å –≤—ã—à–µ –¥–≤–µ—Ä–∏
            return true
        end
    end
    return false
end

-- –ê–≤—Ç–æ—Ç–µ–ª–µ–ø–æ—Ä—Ç –æ—Ç 0 –¥–æ 100 –¥–≤–µ—Ä–µ–π
spawn(function()
    for i = 0, 100 do
        local success = teleportToDoor(i)
        if success then
            print("–¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –¥–≤–µ—Ä–∏ " .. i)
        else
            print("–î–≤–µ—Ä—å " .. i .. " –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")
        end
        wait(1) -- –ø–∞—É–∑–∞ 1 —Å–µ–∫—É–Ω–¥–∞ –º–µ–∂–¥—É —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞–º–∏
    end
    print("–ê–≤—Ç–æ—Ç–µ–ª–µ–ø–æ—Ä—Ç –∑–∞–≤–µ—Ä—à—ë–Ω!")
end)
end)

