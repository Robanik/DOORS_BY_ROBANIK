AddButton("Esp", "Esp Closet", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø GARDR–ûBE
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local camera = Workspace.CurrentCamera

-- –°–æ–∑–¥–∞–µ–º ESP —Ç–µ–∫—Å—Ç
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(0, 255, 255)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 18
    label.Font = Enum.Font.FredokaOne -- –∫—Ä–∞—Å–∏–≤—ã–π —à—Ä–∏—Ñ—Ç

    billboard.Parent = object
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –≤—Å–µ—Ö –∫–æ–º–Ω–∞—Ç
local function setupESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Assets") then
            local assets = room.Assets
            if assets:FindFirstChild("Wardrobe") then
                local wardrobe = assets.Wardrobe:FindFirstChild("Main")
                if wardrobe and not wardrobe:FindFirstChild("BillboardGui") then
                    createBillboard(wardrobe, "(Closet | " .. i .. ")")
                end
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupESP()
    end
end)
end)

AddButton("Esp", "Esp Doors", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø –î–í–ï–†–ï–ô
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –°–æ–∑–¥–∞–µ–º ESP —Ç–µ–∫—Å—Ç
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 4, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 170, 0) -- –æ—Ä–∞–Ω–∂–µ–≤—ã–π (–∫–∞–∫ —Å–≤–µ—Ç —É –¥–≤–µ—Ä–∏)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne -- —Å—Ç–∏–ª—å–Ω—ã–π —à—Ä–∏—Ñ—Ç

    billboard.Parent = object
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –¥–≤–µ—Ä–µ–π
local function setupDoorESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local doorModel = room.Door:FindFirstChild("Door")
            if doorModel and not doorModel:FindFirstChild("BillboardGui") then
                createBillboard(doorModel, "(Door | " .. i .. ")")
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupDoorESP()
    end
end)
end)

AddButton("Esp", "Esp Key", function()
-- // –†–ê–ë–û–ß–ò–ô ESP –î–õ–Ø –í–°–ï–• –ö–õ–Æ–ß–ï–ô
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text, color)
    if object and object:IsA("BasePart") and not object:FindFirstChild("BillboardGui") then
        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = object
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true

        local label = Instance.new("TextLabel")
        label.Parent = billboard
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = text
        label.TextColor3 = color or Color3.fromRGB(0, 255, 0)
        label.TextStrokeTransparency = 0.2
        label.TextSize = 20
        label.Font = Enum.Font.FredokaOne

        billboard.Parent = object
    end
end

-- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫ –≤—Å–µ—Ö –æ–±—ä–µ–∫—Ç–æ–≤ –∫–ª—é—á–µ–π –≤ –∫–æ–º–Ω–∞—Ç–µ
local function findKeys(parent)
    for _, child in pairs(parent:GetChildren()) do
        -- –ï—Å–ª–∏ –æ–±—ä–µ–∫—Ç –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è KeyHitbox
        if child.Name == "KeyHitbox" and child:IsA("BasePart") then
            createBillboard(child, "(Key)", Color3.fromRGB(0, 255, 0))
        end

        -- –ï—Å–ª–∏ –æ–±—ä–µ–∫—Ç —Å–æ–¥–µ—Ä–∂–∏—Ç KeyObtain –∏ –µ—Å—Ç—å Hitbox –≤–Ω—É—Ç—Ä–∏
        if child.Name == "KeyObtain" and child:IsA("Folder") or child:IsA("Model") then
            local hitbox = child:FindFirstChild("Hitbox")
            if hitbox and hitbox:FindFirstChild("KeyHitbox") then
                createBillboard(hitbox.KeyHitbox, "(Key)", Color3.fromRGB(0, 200, 255))
            end
        end

        -- –†–µ–∫—É—Ä—Å–∏–≤–Ω–æ –ø—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ –¥–æ—á–µ—Ä–Ω–∏–µ –æ–±—ä–µ–∫—Ç—ã
        findKeys(child)
    end
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è ESP
local function setupKeyESP()
    if not Workspace:FindFirstChild("CurrentRooms") then return end

    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room then
            findKeys(room)
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 1 —Å–µ–∫—É–Ω–¥—É
task.spawn(function()
    while true do
        setupKeyESP()
        task.wait(1)
    end
end)
end)

AddButton("Esp", "Esp Lever", function()
-- // ESP –î–õ–Ø LEVER (–Ω–µ —Å–≤—è–∑–∞–Ω–Ω—ã–π —Å –∫–ª—é—á–∞–º–∏)
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text, color)
    if object:FindFirstChild("BillboardGui") then return end
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 2, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = color or Color3.fromRGB(255, 165, 0) -- –æ—Ä–∞–Ω–∂–µ–≤—ã–π –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne

    billboard.Parent = object
end

-- –ù–æ–≤—ã–π ESP –¥–ª—è LeverForGate.Main
local function setupLeverESP()
    if not Workspace:FindFirstChild("CurrentRooms") then return end

    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Assets") then
            local lever = room.Assets:FindFirstChild("LeverForGate")
            if lever and lever:FindFirstChild("Main") then
                createBillboard(lever.Main, "(Lever | "..i..")", Color3.fromRGB(255, 165, 0))
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 1 —Å–µ–∫—É–Ω–¥—É
task.spawn(function()
    while true do
        setupLeverESP()
        task.wait(1)
    end
end)
end)

AddButton("Esp", "Esp Lock", function()
-- // –°–¢–ò–õ–¨–ù–´–ô ESP –î–õ–Ø LOCK
-- –ê–≤—Ç–æ—Ä: —Ç–≤–æ–π —Å–∫—Ä–∏–ø—Ç–µ—Ä üòé

local Workspace = game:GetService("Workspace")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createBillboard(object, text)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = object
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 50, 50) -- –∫—Ä–∞—Å–Ω—ã–π (–ø–æ–¥ –∑–∞–º–æ–∫)
    label.TextStrokeTransparency = 0.2
    label.TextSize = 20
    label.Font = Enum.Font.FredokaOne

    billboard.Parent = object
end

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—Å–µ—Ö –∫–æ–º–Ω–∞—Ç
local function setupLockESP()
    for i = 0, 100 do
        local room = Workspace.CurrentRooms:FindFirstChild(tostring(i))
        if room and room:FindFirstChild("Door") then
            local door = room.Door
            if door:FindFirstChild("Lock") then
                local lock = door.Lock
                if lock and not lock:FindFirstChild("BillboardGui") then
                    createBillboard(lock, "(Lock | " .. i .. ")")
                end
            end
        end
    end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã
task.spawn(function()
    while task.wait(1) do
        setupLockESP()
    end
end)
end)
